ut type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#5eeebb\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"checkbox\" title=\"Gradient Overlay\" name=\"gradientOverlay\" checked condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color 2\" name=\"overlayColor2\" value=\"#51aafa\" condition=\"gradientOverlay && overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity}\">\n    </div>\n    \n    <div class=\"animation__background\" mbr-if=\"showAnimation\">\n        <div class=\"circle1\"></div>\n        <div class=\"circle2\"></div>\n        <div class=\"circle3\"></div>\n        <div class=\"circle4\"></div>\n        <div class=\"circle5\"></div>\n    </div>\n    \n    <div class=\"circle__background\" mbr-if=\"showBgCircle\">\n        <div class=\"circle6\"></div>\n        <div class=\"circle7\"></div>\n    </div>\n    \n    <div class=\"container\">\n        <div class=\"row\" mbr-list mbr-list-grow=\"cardCount\">\n            <div class=\"card__wrap col-lg-4 col-md-6 col-sm-6\">\n                <div class=\"card\">\n                    <div class=\"image__card\">\n                        <img src=\"@PROJECT_PATH@/assets/images/-966x491.png\" alt title>\n                         <div class=\"card__info\">\n                            <h4 class=\"card__title mbr-fonts-style\" mbr-if=\"showCardTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".card__title\">Jessica White</h4>\n                            <p class=\"card__text mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card__text\">Teacher</p>\n                        </div>\n        